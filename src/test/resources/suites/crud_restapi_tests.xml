<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Connector-2.0 API Tests">
    <test name="CRUD Appointments Tests" parallel="methods" thread-count="1" preserve-order="false">
        <groups>
            <run>
                <exclude name="excluded"/>
            </run>
        </groups>
        <classes>
            <class name="com.timetrade.connector2.qa.CrudAppointmentsTest"/>
        </classes>
    </test>

    <test name="CRUD Appointments in Batches Tests" parallel="methods" thread-count="1" preserve-order="false">
        <groups>
            <run>
                <exclude name="excluded"/>
            </run>
        </groups>
        <classes>
            <class name="com.timetrade.connector2.qa.CreateAppointmentsInBatchTest"/>
        </classes>
    </test>
</suite>